<%- include("../user/layouts/user_header.ejs") %>


<div class="row ">
  <!-- <div class="col-md-8 col-lg-10"> -->
                     <div class="col-lg-12">
                         <table class="table table-cart table-mobile">
                             <thead>
                                 <tr>
                                   
                                   <th>No </th>
                                   <th>Order ID</th>
                                   <th>Order Date</th>
                                   <th>Total Amount</th>
                                   <th>Payment</th>
                          
                                   <th>View</th>

                                   <th>action</th>
                                 </tr>
                             </thead>
                            
                             <tbody>
                              <% if(orders.length > 0) {
                                orders.forEach((orders, index) => { %>
                                  
                                   <tr>
                                   <td><%= index +1 %>  </td>
                                         
                                       <td> <%= orders.uniqId%></td>
                                       <td><%= orders.date.toLocaleDateString('en-US', { year: 'numeric', month: 'short', day: '2-digit' }).replace(/\//g, '-') %></td>
                                     
                                       
                                   <td><%= orders.totalAmount%></td>
                                   <td><%= orders.paymentMethod %></td>
                              

                                   <!-- <td><%= orders.status%></td> -->
                                   <td>
                                     <a class="btn btn-outline-primary-2" href="/viewProducts?id=<%= orders._id %>">VIEW</a>
                                   </td>
                                 </tr>
                                   <% }) %>
                               <%  }else{ %>

                                <% }%>

                             </tbody>
                            
                         </table><!-- End .table table-wishlist -->
                         
                       
                     </div><!-- End .col-lg-9 -->
                     <!--</div> End .col-lg-9 -->
                     </div><!-- End .col-lg-9 -->






<%- include("../user/layouts/user_footer.ejs") %>

